      <dependency id="Microsoft.SqlServer.ConnectionInfo.dll" version="1.0.1" />
      <dependency id="Microsoft.SqlServer.Management.Sdk.Sfc.dll" version="1.0.1" />
      <dependency id="Microsoft.SqlServer.Smo.dll" version="1.0.1" />
      <dependency id="Microsoft.SqlServer.SqlEnum.dll" version="1.0.1" />
      <dependency id="Microsoft.SqlServer.SqlManagementObjects" version="161.47027.0" />
      <dependency id="System.Data.SqlClient" version="4.8.5" />
    </dependencies>
  </metadata>
  <files>
